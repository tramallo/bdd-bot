    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tramallo/bdd-discord-bot/issues"
    },
    "homepage": "https://github.com/tramallo/bdd-discord-bot#readme",
    "dependencies": {
        "discord.js": "^13.8.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.41",
        "typescript": "^4.7.3"
